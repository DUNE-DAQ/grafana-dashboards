          "hide": false,
          "measurement": "dunedaq.timing.timingfirmwareinfo.MasterMonitorData",
          "orderByTime": "ASC",
          "policy": "default",
          "query": "SELECT derivative(mean(\"timestamp\"), 1s) AS \"Timestamp change\" FROM \"dunedaq.timing.timingfirmwareinfo.MasterMonitorData\" WHERE (\"partition_id\" =~ /^$partition$/) AND time >= now() - 3h - 1m and time <= now() GROUP BY time(1m), \"partition_id\"",
          "rawQuery": false,
          "refId": "B",
          "resultFormat": "time_series",
          "select": [
            [
              {
                "params": [
                  "timestamp"
                ],
                "type": "field"
              },
              {
                "params": [],
                "type": "mean"
              },
              {
                "params": [
                  "1s"
                ],
                "type": "derivative"
              },
              {
                "params": [
                  "Timestamp change"
                ],
                "type": "alias"
              }
            ]
          ],
          "tags": [
            {
              "key": "partition_id",
              "operator": "=~",
              "value": "/^$partition$/"
            }
          ]
        }
