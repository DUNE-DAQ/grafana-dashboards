          "query": "SELECT \"timestamp\" / 62500000 AS \"Timestamp\" FROM \"dunedaq.timing.timingfirmwareinfo.MasterMonitorData\" WHERE (\"partition_id\" =~ /^$partition$/) AND $timeFilter GROUP BY \"partition_id\"",
          "rawQuery": true,
          "refId": "A",
          "resultFormat": "time_series",
          "select": [
            [
              {
                "params": [
                  "timestamp"
                ],
                "type": "field"
