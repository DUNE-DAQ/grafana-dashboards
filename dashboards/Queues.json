              {
                "params": [
                  "number_of_elements"
                ],
                "type": "field"
              },
              {
                "params": [],
                "type": "distinct"
              },
              {
                "params": [
                  " * 100"
                ],
                "type": "math"
              }
            ]
          ],
          "tags": [
            {
              "key": "source_id",
              "operator": "=",
              "value": "maroda_test_dataflow.trigger_record_q"
            }
          ]
        }
      ],
      "title": "Queue occupancy for application: ${application}",
      "transformations": [
